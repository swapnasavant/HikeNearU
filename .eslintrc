{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "max-len": 0,
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0,
    no-console: ["error", { allow: ["warn", "error"] }],
  }
}
